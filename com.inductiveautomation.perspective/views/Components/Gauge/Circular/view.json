{
  "custom": {
    "cssVariable": "var(--black)",
    "secondaryCssVariable": "var(--grey-lighten-2)"
  },
  "params": {
    "color": "primary",
    "endDeg": 315,
    "fontSize": 20,
    "gap": 0,
    "max": 100,
    "min": 0,
    "rotate": 0,
    "secondaryColor": "background",
    "setpoint": 50,
    "startDeg": 45,
    "units": "%",
    "value": 70
  },
  "propConfig": {
    "custom.cssVariable": {
      "binding": {
        "config": {
          "struct": {
            "color": "{view.params.color}",
            "selectedTheme": "{session.custom.framework.selectedTheme}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tif value[\u0027color\u0027] in self.session.custom.framework.selectedSettings:\n\t\tvariable \u003d self.session.custom.framework.variables[value[\u0027color\u0027]]\n\t\treturn \u0027var(%s)\u0027 % (variable,)\n\telse:\n\t\tif theme.isValidParam(value[\u0027color\u0027]):\n\t\t\treturn \u0027var(%s)\u0027 % (theme.getVariable(value[\u0027color\u0027]),)\n\t\telse:\n\t\t\treturn value[\u0027color\u0027]",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.secondaryCssVariable": {
      "binding": {
        "config": {
          "struct": {
            "color": "{view.params.secondaryColor}",
            "selectedTheme": "{session.custom.framework.selectedTheme}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tif value[\u0027color\u0027] in self.session.custom.framework.selectedSettings:\n\t\tvariable \u003d self.session.custom.framework.variables[value[\u0027color\u0027]]\n\t\treturn \u0027var(%s)\u0027 % (variable,)\n\telse:\n\t\tif theme.isValidParam(value[\u0027color\u0027]):\n\t\t\treturn \u0027var(%s)\u0027 % (theme.getVariable(value[\u0027color\u0027]),)\n\t\telse:\n\t\t\treturn value[\u0027color\u0027]",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "params.color": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.endDeg": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.fontSize": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.gap": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.max": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.min": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.rotate": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.secondaryColor": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.setpoint": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.startDeg": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.units": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 100,
      "width": 100
    }
  },
  "root": {
    "children": [
      {
        "custom": {
          "circumference": 125.664
        },
        "meta": {
          "name": "gauge"
        },
        "position": {
          "height": "100%",
          "width": "100%"
        },
        "propConfig": {
          "custom.endDeg": {
            "binding": {
              "config": {
                "path": "view.params.gap"
              },
              "transforms": [
                {
                  "expression": "360-{value}",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "custom.startDeg": {
            "binding": {
              "config": {
                "path": "view.params.gap"
              },
              "type": "property"
            }
          },
          "props.elements[0].elements[0].stroke.dasharray": {
            "binding": {
              "config": {
                "expression": "((({this.custom.endDeg} - {this.custom.startDeg}))/360)*{this.custom.circumference}"
              },
              "type": "expr"
            }
          },
          "props.elements[0].elements[0].stroke.dashoffset": {
            "binding": {
              "config": {
                "expression": "-1 * ({this.custom.startDeg} / 360) * {this.custom.circumference}"
              },
              "type": "expr"
            }
          },
          "props.elements[0].elements[0].stroke.paint": {
            "binding": {
              "config": {
                "path": "view.custom.secondaryCssVariable"
              },
              "type": "property"
            }
          },
          "props.elements[0].elements[0].style.transform": {
            "binding": {
              "config": {
                "path": "view.params.rotate"
              },
              "transforms": [
                {
                  "expression": "\u0027rotate(\u0027+{value}+\u0027deg)\u0027",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.elements[0].elements[1].stroke.dasharray": {
            "binding": {
              "config": {
                "expression": "((({view.params.value} - {view.params.min})/({view.params.max} - {view.params.min}) \n* ({this.custom.endDeg} - {this.custom.startDeg}))/360)*{this.custom.circumference}"
              },
              "transforms": [
                {
                  "expression": "{value} + \u0027 \u0027 + {this.custom.circumference}",
                  "type": "expression"
                }
              ],
              "type": "expr"
            }
          },
          "props.elements[0].elements[1].stroke.dashoffset": {
            "binding": {
              "config": {
                "expression": "-1 * ({this.custom.startDeg} / 360) * {this.custom.circumference}"
              },
              "type": "expr"
            }
          },
          "props.elements[0].elements[1].stroke.paint": {
            "binding": {
              "config": {
                "path": "view.custom.cssVariable"
              },
              "type": "property"
            }
          },
          "props.elements[0].elements[1].style.transform": {
            "binding": {
              "config": {
                "path": "view.params.rotate"
              },
              "transforms": [
                {
                  "expression": "\u0027rotate(\u0027+{value}+\u0027deg)\u0027",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.elements[0].elements[2].elements[0].fill": {
            "binding": {
              "config": {
                "path": "view.custom.cssVariable"
              },
              "type": "property"
            }
          },
          "props.elements[0].elements[2].style.transform": {
            "binding": {
              "config": {
                "expression": "({view.params.setpoint} - {view.params.min})\n/({view.params.max} - {view.params.min})"
              },
              "transforms": [
                {
                  "expression": "{view.params.rotate} + {this.custom.startDeg} + {value} * ({this.custom.endDeg} - {this.custom.startDeg})",
                  "type": "expression"
                },
                {
                  "expression": "\u0027rotate(\u0027 + \ntoStr({value})\n+ \u0027deg)\u0027",
                  "type": "expression"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
          "elements": [
            {
              "elements": [
                {
                  "cx": "45.714285714285715",
                  "cy": "45.714285714285715",
                  "fill": {
                    "paint": "transparent"
                  },
                  "name": "circle",
                  "r": 20,
                  "stroke": {
                    "width": 5.7143
                  },
                  "style": {
                    "transformOrigin": "45.714px 45.714px",
                    "transition": "all 0.6s ease-in-out"
                  },
                  "type": "circle"
                },
                {
                  "cx": "45.714285714285715",
                  "cy": "45.714285714285715",
                  "name": "circle",
                  "r": 20,
                  "stroke": {
                    "width": 5.7143
                  },
                  "style": {
                    "transformOrigin": "45.714px 45.714px",
                    "transition": "all 0.6s ease-in-out"
                  },
                  "type": "circle"
                },
                {
                  "elements": [
                    {
                      "d": "M 45.714 28.59 L 47.214 31.59 L 44.214 31.59 L 45.714 28.59 Z",
                      "transform": "matrix(0, 1, -1, 0, 91.389999, 0.000285)",
                      "type": "path"
                    }
                  ],
                  "style": {
                    "transformOrigin": "45.714285714285715px 45.714285714285715px",
                    "transition": "all .6s ease-in-out"
                  },
                  "type": "group"
                }
              ],
              "fill": {
                "paint": "none"
              },
              "name": "group",
              "transform": "transform: rotate(0deg)",
              "type": "group"
            }
          ],
          "viewBox": "22.86 22.86 45.714 45.714"
        },
        "type": "ia.shapes.svg"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "height": "100%",
          "width": "100%"
        },
        "propConfig": {
          "props.style.color": {
            "binding": {
              "config": {
                "path": "view.custom.cssVariable"
              },
              "type": "property"
            }
          },
          "props.style.fontSize": {
            "binding": {
              "config": {
                "path": "view.params.fontSize"
              },
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "expression": "numberFormat({view.params.value},\"#,##0.#\") + \u0027 \u0027 + {view.params.units}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "classes": "typo/font/oswald",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.coord"
  }
}